@using K9.Base.WebApplication.Helpers
@model ProjectDetail
@{
	Layout = "~/Views/Shared/_LayoutFormWide.cshtml";
}
@Html.BootstrapTable(new DataTableOptions<ProjectDetail>
{
    VisibleColumns = new List<string>
    {
        "NumberOfIbogasSponsoredToDate",
        "NumberOfIbogasPlantedToDate",
        "NumberOfIbogasProjectedToBePlantedThisYear",
        "NumberOfIbogasProjectedToBePlantedTwoYears"
    },
    AllowCreate = true,
    AllowDelete = false
})